{
  "manifest_version":2,
  "name":"Shu",
  "version":"1.0.0",
  "description":"A chrome extention that enables developers to make utility plugins for jitsimeet based systems like shkolo.bg. The name comes from SHkolo Utilities.",
  "content_scripts":[
    {
      "matches":["https://meet.jit.si/*"],
      "js":["content.js"],
      "all_frames": true
    },
    {
      "matches":["https://*.shkolo.live/*"],
      "js":["content.js"],
      "all_frames": true
    }
  ],
  "background":{
    "scripts":["background.js"]
  },
  "browser_action":{
    "default_icon":"icon.png"
  }
}